{
  "name": "implobot",
  "version": "1.0.0",
  "scripts": {
    "start-docker-dev": "docker-compose -f docker-compose-dev.yml up -d --build",
    "start-docker-deploy": "docker-compose -f docker-compose-deploy.yml up -d --build",
    "stop-docker-dev": "docker-compose -f docker-compose-dev.yml down",
    "stop-docker-deploy": "docker-compose -f docker-compose-deploy.yml down",
    "tsc-watch": "tsc-watch ./src/index.ts --outDir ./dist --onSuccess \"node --inspect=0.0.0.0:9229 dist/\"",
    "tsc-compile": "tsc ./src/index.ts --project tsconfig.json",
    "node-run": "node ./dist/index.js",
    "tsc-run": "tsc-watch ./src/index.ts --outDir ./dist --onSuccess \"node dist/\""
  },
  "dependencies": {
    "@types/node": "^13.13.4",
    "discord.js": "^12.2.0",
    "eris": "^0.11.2",
    "mysql": "^2.18.1",
    "ts-node": "^8.9.1",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "description": "bot for the implodium server https://discord.gg/hmHnb8n",
  "main": "./src/index.ts",
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuirinEcker/implo-bot.git"
  },
  "keywords": [
    "discord",
    "github",
    "bot",
    "docker",
    "node",
    "typescript",
    "postgres"
  ],
  "author": "QuirinEcker",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/QuirinEcker/implo-bot/issues"
  },
  "homepage": "https://github.com/QuirinEcker/implo-bot#readme"
}
